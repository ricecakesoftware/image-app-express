{
  "name": "image-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "node dist/server/index.js",
    "lint": "next lint"
  },
  "nodemonConfig": {
    "watch": [
      "server"
    ],
    "exec": "npx ts-node --project tsconfig.server.json server/index.ts",
    "ext": "js ts"
  },
  "dependencies": {
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "socket.io": "^4.4.1",
    "socket.io-client": "^4.4.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/formidable": "^2.0.4",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "17.0.21",
    "@types/react": "17.0.40",
    "eslint": "8.11.0",
    "eslint-config-next": "12.1.0",
    "formidable": "^2.0.1",
    "nodemon": "^2.0.15",
    "ts-node": "^10.7.0",
    "typescript": "4.6.2"
  }
}
